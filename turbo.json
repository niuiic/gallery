{
  "$schema": "https://turbo.build/schema.json",
  "ui": "stream",
  "tasks": {
    "website#dev": {
      "dependsOn": ["css#build", "webgl#build"]
    },
    "website#build": {
      "dependsOn": ["css#build", "webgl#build"]
    },
    "css#dev": {
      "dependsOn": ["farm-plugins#build"]
    },
    "css#build": {
      "dependsOn": ["farm-plugins#build"]
    },
    "webgl#dev": {
      "dependsOn": ["farm-plugins#build"]
    },
    "webgl#build": {
      "dependsOn": ["farm-plugins#build"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}
